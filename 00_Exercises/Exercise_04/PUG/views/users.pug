doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="css/main.css")
        title #{title}
    body 
        h1 List of Users:
        each user in users 
            .usuarios 
                h2 Name: #{user.name}
                p id: #{user.id}